

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
   <hibernate-mapping>
   
   
   <class name="com.sp.entity.Account" table="accou">
   
     <id name="account_num" column="account_num"/>
     <property name="first_name" column="first_name"/>
     <property name="last_name" column="last_name"/>
     <property name="account_balance" column="account_balance"/>
     <property name="city" column="city"/>
		

   </class>
   
   </hibernate-mapping>